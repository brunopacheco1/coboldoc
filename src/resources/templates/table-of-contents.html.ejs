<!DOCTYPE>
<html lang="en" xml:lang="en">

<head>
    <title>Table of Contents</title>
    <meta content="text/html; charset=utf-8" />
</head>

<body>
    <h1>
        Table of Contents
    </h1>
    <ul>
        <% fileNames.forEach((fileName) => { %><li>
            <a href="./<%- fileName %>"><%- fileName.substring(0, fileName.lastIndexOf('.')) %></a>
        </li><% }); %>
    </ul>
</body>

</html>